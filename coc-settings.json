{

    //suggest
    "suggest.enablePreview": true,
    "suggest.autoTrigger": "always",
    "suggest.languageSourcePriority": 99,
    "suggest.enablePreselect": true,
    "suggest.noselect": true,
    "suggest.numberSelect": false,
    "suggest.timeout": 500,
    "suggest.detailField": "preview",
    "suggest.snippetIndicator": "",
    "suggest.completionItemKindLabels": {
        "keyword": "\uf1de",
        "variable": "\ue79b",
        "value": "\uf89f",
        "operator": "\u03a8",
        "function": "\u0192",
        "reference": "\ufa46",
        "constant": "\uf8fe",
        "method": "\uf09a",
        "struct": "\ufb44",
        "class": "\uf0e8",
        "interface": "\uf417",
        "text": "\ue612",
        "enum": "\uf435",
        "enumMember": "\uf02b",
        "module": "\uf40d",
        "color": "\ue22b",
        "property": "\ue624",
        "field": "\uf9be",
        "unit": "\uf475",
        "event": "\ufacd",
        "file": "\uf723",
        "folder": "\uf114",
        "snippet": "\ue60b",
        "typeParameter": "\uf728",
        "default": "\uf29c"
    },

    //diagnostic
    "diagnostic.signOffset": 9999999,
    "diagnostic.level": "information",
    //"diagnostic.errorSign": "猪",
    //"diagnostic.warningSign": "警",
    "diagnostic.displayByAle": false,
    "diagnostic.refreshOnInsertMode": true,

    //proxy
    "http.proxy": "http://127.0.0.1:8090",

    //git
    "git.enableGutters": true,
    "git.branchCharacter": "\uf408",
    "git.addGBlameToBufferVar": true,
    "git.addGBlameToVirtualText": true,
    "git.virtualTextPrefix": " -> ",
    "git.addedSign.hlGroup": "GitGutterAdd",
    "git.changedSign.hlGroup": "GitGutterChange",
    "git.removedSign.hlGroup": "GitGutterDelete",
    "git.topRemovedSign.hlGroup": "GitGutterDelete",
    "git.changeRemovedSign.hlGroup": "GitGutterChangeDelete",

    //Snippet
    "coc.preferences.snippetStatusText": "Ⓢ ",
    "snippets.priority": 99,



    //python
    "python.jediEnabled": false,

    //yank
    "yank.enableCompletion": false,


    //list
    "list.source.grep.command": "rg",
    "list.source.files.args": ["--hidden", "--files"],
    "list.source.files.defaultOptions": ["--ignore-case"],
    "list.source.outline.defaultOptions": ["--auto-preview"],


    //coc-flow

    //session
    "session.directory": "~/.cache/vim/session",
    //multiple curssors
    "cursors.nextKey": "<c-n>",
    "cursors.previousKey": "<c-p>",
    "cursors.cancelKey": "<esc>",

    //language
    "list.previewSplitRight": true,

    "languageserver": {
        "clangd": {
            "command": "clangd",
            "args": ["--background-index"],
            "rootPatterns": [
                "compile_flags.txt",
                "compile_commands.json",
                ".vim/",
                ".git/",
                ".hg/"
            ],
            "filetypes": ["c", "cpp", "objc", "objcpp"]
        }
    },

    //eslint
    //"eslint.filetypes": ["javascript", "javascript.jsx", "javascriptreact"],
    //"eslint.autoFix": true,
    //"eslint.autoFixOnSave": true,

    //emmet
    "emmet.includeLanguages": {
        "vue-html": "html",
        "javascript": "javascriptreact"
    },

    //prettier
    //"coc.preferences.formatOnSaveFiletypes": ["css", "markdown"],
    //"prettier.statusItemText": "ⓟ ",
    //"prettier.eslintIntegration": true,
    //"prettier.tslintIntegration": true,
    //"prettier.stylelintIntegration": true,
    "prettier.singleQuote": true,
    "prettier.disableLanguages": [
        "vue",
        "python"
    ],

    //smartf
    "smartf.wordJump": false,

    //coc-tailwindcss
    "tailwindCSS.trace.server": "verbose",
    "tailwindCSS.emmetCompletions": true,


    //kite
    "kite.enable": true,
    "kite.startKiteEngineOnStartup": true,
    "kite.enableSnippets": false

    //java

}
