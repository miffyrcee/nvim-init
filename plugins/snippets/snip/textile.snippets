# Jekyll post header
snippet header
	---
	title: ${1:title}
	layout: post
	date: ${2:date} ${0:hour:minute:second} -05:00
	---

# Image
endsnippet
snippet img
	!${1:url}(${2:title}):${0:link}!

# Table
endsnippet
snippet |
	|${1}|

# Link
endsnippet
snippet link
	"${1:link text}":${0:url}

# Acronym
endsnippet
snippet (
	(${1:Expand acronym})

# Footnote
endsnippet
snippet fn
	[${1:ref number}] ${0}

	fn$1. ${2:footnote}

endsnippet
