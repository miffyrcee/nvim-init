snippet pipeline_sample
	pipeline {
		agent any 
		stages {
			stage('${1:Build}') { 
				steps { 
					sh '${2:make}' 
				}
			}
			stage('${3:Test}'){
				steps {
					sh '${4:make check}'
					junit '${5:reports/**/*.xml}' 
				}
			}
			stage('${6:Deploy}') {
				steps {
					sh '${7:make publish}'
				}
			}
		}
	}
	${0}

endsnippet
